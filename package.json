    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.5.0",
    "leasot": "^6.3.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.6",
    "sonar-scanner": "^3.1.0"
  },
  "dependencies": {
    "jpeg-js": "^0.3.4",
    "pixelmatch": "^4.0.2"
  }
